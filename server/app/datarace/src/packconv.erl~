-module(packconv).

-export([]).
-record(login_data,{User_name, Password})


convert_pack(Data)->
    List_data = binary_to_list(Data),
    case hd(List_data) of
	0 ->
	    login_pack(List_data)
    end.
    

login_pack(List)->
    User_name = lists:sublist(List,0,50)
    Password =


list_to_data([], 0)-> [];
list_to_data([0|T], Max_len)-> list_to_data(T, Max_len -1).
list_to_data([H|T], Max_len)-> [H|list_to_data(T, Max_len -1)].
